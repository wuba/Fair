{
  "className": "Scaffold",
  "na": {
    "appBar": {
      "className": "AppBar",
      "na": {
        "title": {
          "className": "Text",
          "pa": [
            "ListLoadMore"
          ]
        }
      }
    },
    "body": {
      "className": "Center",
      "na": {
        "child": {
          "className": "RefreshIndicator",
          "na": {
            "displacement": 10.0,
            "onRefresh": "@(_onRefresh)",
            "child": {
              "className": "Sugar.ifEqualBool",
              "pa": [
                "%(listIsEmpty)"
              ],
              "na": {
                "trueValue": {
                  "className": "Center",
                  "na": {
                    "child": {
                      "className": "CircularProgressIndicator"
                    }
                  }
                },
                "falseValue": {
                  "className": "ListView.builder",
                  "na": {
                    "controller": "^(_scrollController)",
                    "itemCount": "%(_itemCount)",
                    "itemBuilder": "@(_itemBuilder)"
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "methodMap": {
    "_itemBuilder": {
      "className": "ListTile",
      "na": {
        "title": {
          "className": "Text"
        }
      }
    }
  }
}