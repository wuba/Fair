{
  "className": "Scaffold",
  "na": {
    "appBar": {
      "className": "AppBar",
      "na": {
        "title": {
          "className": "Text",
          "pa": [
            "^(_name)"
          ]
        }
      }
    },
    "body": {
      "className": "Center",
      "na": {
        "child": {
          "className": "Column",
          "na": {
            "mainAxisAlignment": "#(MainAxisAlignment.center)",
            "children": [
              {
                "className": "Image.asset",
                "pa": [
                  "assets/image/logo.png"
                ]
              },
              {
                "className": "Padding",
                "na": {
                  "padding": {
                    "className": "EdgeInsets.only",
                    "na": {
                      "bottom": 40
                    }
                  },
                  "child": {
                    "className": "Text",
                    "pa": [
                      "#($_count)"
                    ]
                  }
                }
              }
            ]
          }
        }
      }
    },
    "floatingActionButton": {
      "className": "Row",
      "na": {
        "mainAxisAlignment": "#(MainAxisAlignment.end)",
        "children": [
          {
            "className": "FloatingActionButton",
            "na": {
              "child": {
                "className": "Icon",
                "pa": [
                  "#(Icons.add)"
                ]
              },
              "heroTag": "add",
              "onPressed": "@(onTapText)"
            }
          },
          {
            "className": "FloatingActionButton",
            "na": {
              "child": {
                "className": "Icon",
                "pa": [
                  "#(Icons.open_in_new)"
                ]
              },
              "heroTag": "open_in_new",
              "onPressed": {
                "className": "FairFunction",
                "body": {
                  "className": "Navigator.pushNamed",
                  "pa": [
                    "^(context)",
                    "fair_page"
                  ],
                  "na": {
                    "arguments": {
                      "name": "动态界面&传值",
                      "path": "assets/fair/lib_src_page_logic-page2page_sample_logic_page2page.fair.json",
                      "data": {
                        "_count": "^(_count)"
                      }
                    }
                  }
                },
                "tag": "void Function()",
                "returnType": "void"
              }
            }
          }
        ]
      }
    }
  },
  "methodMap": {
    "createState": "%(_Page2PageState)"
  }
}