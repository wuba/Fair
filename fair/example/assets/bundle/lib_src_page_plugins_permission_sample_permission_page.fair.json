{
  "className": "MaterialApp",
  "na": {
    "home": {
      "className": "Scaffold",
      "na": {
        "appBar": {
          "className": "AppBar",
          "na": {
            "title": {
              "className": "Text",
              "pa": [
                "图片选择"
              ]
            }
          }
        },
        "body": {
          "className": "Container",
          "na": {
            "child": {
              "className": "Column",
              "na": {
                "children": [
                  {
                    "className": "Sugar.ifEqual",
                    "pa": [
                      "#($picUrl)",
                      ""
                    ],
                    "na": {
                      "trueValue": {
                        "className": "Image.network",
                        "pa": [
                          "https://img.58cdn.com.cn/logo/58/252_84/logo-o.png"
                        ],
                        "na": {
                          "width": 1000,
                          "height": 500
                        }
                      },
                      "falseValue": {
                        "className": "Image.file",
                        "pa": [
                          {
                            "className": "File",
                            "pa": [
                              "^(picUrl)"
                            ]
                          }
                        ],
                        "na": {
                          "width": 1000,
                          "height": 500
                        }
                      }
                    }
                  },
                  {
                    "className": "Text",
                    "pa": [
                      "点击按钮拍照"
                    ]
                  },
                  {
                    "className": "MaterialButton",
                    "na": {
                      "color": "#(Colors.grey)",
                      "onPressed": "@(requestPermission)",
                      "child": {
                        "className": "Text",
                        "pa": [
                          "相机"
                        ],
                        "na": {
                          "style": {
                            "className": "TextStyle",
                            "na": {
                              "fontSize": 19
                            }
                          }
                        }
                      }
                    }
                  },
                  {
                    "className": "MaterialButton",
                    "na": {
                      "color": "#(Colors.grey)",
                      "onPressed": "@(selectFromAlbum)",
                      "child": {
                        "className": "Text",
                        "pa": [
                          "相册"
                        ],
                        "na": {
                          "style": {
                            "className": "TextStyle",
                            "na": {
                              "fontSize": 19
                            }
                          }
                        }
                      }
                    }
                  }
                ]
              }
            }
          }
        }
      }
    }
  },
  "methodMap": {}
}