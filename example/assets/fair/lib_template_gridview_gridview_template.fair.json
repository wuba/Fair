{
  "className": "Scaffold",
  "na": {
    "appBar": {
      "className": "AppBar",
      "na": {
        "title": {
          "className": "Text",
          "pa": [
            "GridView模版"
          ]
        }
      }
    },
    "floatingActionButton": {
      "className": "FloatingActionButton",
      "na": {
        "onPressed": "@(requestData)",
        "child": {
          "className": "Icon",
          "pa": [
            "#(Icons.add)"
          ]
        }
      }
    },
    "body": {
      "className": "Sugar.ifEqualBool",
      "pa": [
        "%(isDataEmpty)"
      ],
      "na": {
        "trueValue": {
          "className": "FairFunction",
          "body": {
            "className": "Center",
            "na": {
              "child": {
                "className": "Text",
                "pa": [
                  "加载中..."
                ]
              }
            }
          },
          "tag": "Widget Function()",
          "rt": "Widget"
        },
        "falseValue": {
          "className": "FairFunction",
          "body": {
            "className": "GridView.count",
            "na": {
              "crossAxisCount": 2,
              "mainAxisSpacing": 10,
              "crossAxisSpacing": 10,
              "children": {
                "className": "Sugar.map",
                "pa": [
                  "^(_listData)"
                ],
                "na": {
                  "builder": {
                    "className": "FairFunction",
                    "body": {
                      "className": "AspectRatio",
                      "na": {
                        "aspectRatio": 1.5,
                        "child": {
                          "className": "ClipRRect",
                          "na": {
                            "borderRadius": {
                              "className": "BorderRadius.all",
                              "pa": [
                                {
                                  "className": "Radius.circular",
                                  "pa": [
                                    4.0
                                  ]
                                }
                              ]
                            },
                            "child": {
                              "className": "Image.network",
                              "pa": [
                                "$(item.picUrl)"
                              ],
                              "na": {
                                "fit": "#(BoxFit.cover)"
                              }
                            }
                          }
                        }
                      }
                    },
                    "parameters": {
                      "pa": [
                        "item"
                      ]
                    },
                    "tag": "Widget Function(ItemData)",
                    "rt": "Widget"
                  }
                }
              }
            }
          },
          "tag": "Widget Function()",
          "rt": "Widget"
        }
      }
    }
  },
  "methodMap": {}
}